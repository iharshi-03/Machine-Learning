from sklearn.tree import DecisionTreeClassifier
import pandas as pd
data = {'Income': [50000,60000,30000,70000], 'Debt': [2000,1000,5000,1500], 'Score': ['Good','Good','Bad','Good']}
df = pd.DataFrame(data)
X = df[['Income','Debt']]
y = df['Score']
model = DecisionTreeClassifier()
model.fit(X, y)
income = float(input("Enter Income: "))
debt = float(input("Enter Debt: "))
print("Predicted Score:", model.predict([[income, debt]])[0])
