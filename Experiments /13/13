from sklearn.linear_model import LinearRegression
import pandas as pd
data = {'Year': [2015,2016,2017,2018], 'Mileage': [30000,25000,20000,15000], 'Price': [20000,22000,24000,26000]}
df = pd.DataFrame(data)
X = df[['Year','Mileage']]
y = df['Price']
model = LinearRegression()
model.fit(X, y)
year = int(input("Enter Year: "))
mileage = int(input("Enter Mileage: "))
print("Predicted Price:", model.predict([[year, mileage]])[0])
