from sklearn.linear_model import LinearRegression
import pandas as pd
data = {'Size': [1500,2000,1200,1800], 'Rooms': [3,4,2,3], 'Price': [300000,400000,250000,350000]}
df = pd.DataFrame(data)
X = df[['Size','Rooms']]
y = df['Price']
model = LinearRegression()
model.fit(X, y)
size = int(input("Enter Size: "))
rooms = int(input("Enter Rooms: "))
print("Predicted Price:", model.predict([[size, rooms]])[0])
